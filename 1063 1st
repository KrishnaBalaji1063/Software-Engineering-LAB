#include <stdio.h>

float predict_temperature(int time) {
    float a = 0.1;
    float b = 0.5;
    float c = 10;
    return a * (time * time) + b * time + c;
}

int main() {
    int input_time = 10;
    float predicted_temperature = predict_temperature(input_time);
    printf("Predicted temperature after %d hours: %.2fÂ°C\n", input_time, predicted_temperature);
    return 0;
}
